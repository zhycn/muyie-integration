            </dependency>
            <dependency>
                <groupId>com.github.zhycn</groupId>
                <artifactId>muyie-integration-data-mybatis</artifactId>
                <version>${muyie-integration.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.zhycn</groupId>
                <artifactId>muyie-integration-data-redis</artifactId>
                <version>${muyie-integration.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.zhycn</groupId>
                <artifactId>muyie-integration-dubbo</artifactId>
                <version>${muyie-integration.version}</version>
            </dependency>
			<dependency>
                <groupId>com.github.zhycn</groupId>
                <artifactId>muyie-integration-flyway</artifactId>
                <version>${muyie-integration.version}</version>
            </dependency>
        </dependencies>
