    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.github.zhycn</groupId>
                <artifactId>muyie-integration-apollo</artifactId>
                <version>${muyie-integration.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.zhycn</groupId>
                <artifactId>muyie-integration-data-jpa</artifactId>
